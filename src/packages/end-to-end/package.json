{
	"name": "@exogee/graphweaver-end-to-end",
	"version": "0.2.6",
	"description": "Graphweaver Test Suite",
	"license": "MIT",
	"scripts": {
		"import-database-sqlite": ". ./scripts/import-sqlite.sh",
		"import-database-postgres": ". ./scripts/import-postgres.sh",
		"import-database-mysql": ". ./scripts/import-mysql.sh",
		"seed-postgres": "node ./scripts/seed-postgres.js",
		"start-server": "cd app && pnpm start",
		"dev-sqlite": "pnpm import-database-sqlite && pnpm start-server",
		"dev-postgres": "createdb gw && pnpm seed-postgres && pnpm import-database-postgres  && pnpm start-server",
		"dev-mysql": "pnpm import-database-mysql && pnpm start-server",
		"test-sqlite": "export DATABASE=sqlite && jest --runInBand --forceExit ---- api/sqlite",
		"test-postgres": "export DATABASE=postgres && jest --runInBand -- api/postgres",
		"test-mysql": "export DATEBASE=mysql && jest --runInBand -- api/mysql",
		"test-ui": "npx playwright test",
		"version": "npm version --no-git-tag-version"
	},
	"dependencies": {
		"@exogee/graphweaver": "*",
		"@exogee/graphweaver-scalars": "*",
		"@exogee/graphweaver-server": "*",
		"graphweaver": "*",
		"@exogee/graphweaver-mikroorm": "*",
		"@mikro-orm/core": "5.7.14",
		"@mikro-orm/sqlite": "5.7.14",
		"reflect-metadata": "0.1.13",
		"type-graphql": "2.0.0-beta.2",
		"class-validator": "0.14.0",
		"graphql": "16.6.0"
	},
	"devDependencies": {
		"@playwright/test": "1.37.1",
		"@types/jest": "26.0.23",
		"graphql": "16.6.0",
		"graphql-tag": "2.12.6",
		"jest": "29.4.0",
		"pg": "8.11.1",
		"supertest-graphql": "1.1.4",
		"ts-jest": "29.0.5",
		"typescript": "5.0.2"
	},
	"keywords": [
		"graphql",
		"gql",
		"headless",
		"cms",
		"postgres",
		"postgresql",
		"mysql",
		"sqlite"
	]
}
