{
	"name": "@exogee/graphweaver-end-to-end",
	"version": "0.0.1",
	"description": "Graphweaver Test Suite",
	"license": "MIT",
	"scripts": {
		"test:init": "rm -rf ./test && node ../cli/bin init --name=test --backend=sqlite version=workspace:*",
		"test:import": "cd test && node ../../cli/bin import sqlite --database=../database.sqlite",
		"test:start": "cd test && pnpm start",
		"test:e2e": "jest"
	},
	"devDependencies": {
		"@types/jest": "26.0.23",
		"graphql": "16.6.0",
		"graphql-tag": "2.12.6",
		"jest": "29.4.0",
		"supertest-graphql": "1.1.4",
		"ts-jest": "29.0.5",
		"typescript": "5.0.2"
	}
}
